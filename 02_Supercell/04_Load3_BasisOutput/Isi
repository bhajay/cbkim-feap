 hist 1 21
 hist 1 22
 hist 1 23
 hist 1 24 
 hist 1 25
 hist 1 26
 hist 1 27
 hist 1 28
 hist 1 29
 hist 1 30
 hist 1 31
 hist 1 32
 hist 1 33
 hist 1 33
 hist 1 34
 hist 1 35
 hist 1 36
 hist 1 37
 hist 1 38
 hist 1 39
 hist 1 40
 hist 1 41
 hist 1 42
 hist 1 43
 hist 1 44
 hist 1 45
 show


batch
 plot,perspective
 plot,node
 plot,hide
 plot,unde
end
0
.5 .5 5
0 1 0



batch
 prop,,1
 dt,,2
end
2
0 0
414 414
828 0

! Run upto step 176 and then save the state
batch
  loop,,176
   time
   loop,,10
    tang,,1
   next
    reac,coor,3,1.0
     plot,defo,1,1
     plot,frame,1
     plot,wipe,1
     plot,stre,3
     plot,boun
     plot,disp	
     plot,node
     plot,frame,2
     plot,wipe,2
     plot,cont,1
     plot,boun
     plot,disp	
     plot,node
     plot,frame,3
     plot,wipe,3
     plot,cont,2
     plot,boun
     plot,disp	
     plot,node
     plot,frame,4
     plot,wipe,4
     plot,cont,3
     plot,boun
     plot,disp	
  next
  save,,176
end


